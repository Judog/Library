<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<Pane fx:id="popupPane" prefHeight="150" prefWidth="150" xmlns:fx="http://javafx.com/fxml/1"
      xmlns="http://javafx.com/javafx/11.0.1" fx:controller="pl.kamilsieczkowski.LoginPopupController">
    <children>
        <VBox alignment="CENTER" prefHeight="150" prefWidth="150" spacing="7">
            <children>
                <Label fx:id="loginStatusText"/>
                <Button fx:id="okButton" text="Ok"/>
            </children>
        </VBox>
    </children>
</Pane>
